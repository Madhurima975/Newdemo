pipeline {
    stages {
        stage('Clone repository') {
            steps {
                git 'https://github.com/Madhurima975/Newdemo.git'
            }
        }
        stage('Build Docker image') {
            steps {
                sh 'docker build -t your-image-name .'
            }
        }
        stage('Run Docker container') {
            steps {
                sh 'docker run -d -p 80:80 your-image-name'
            }
        }
    }
}
